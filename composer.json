{
  "name": "allansun/openapi-parser",
  "description": "A universal OpenAPI (Swagger) schema parser",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Allan Sun",
      "email": "allan.sun@bricre.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "ext-fileinfo": "*",
    "symfony/yaml": "^v5|^v6|^v7",
    "symfony/property-info": "^v5|^v6|^v7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpdocumentor/reflection-docblock": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "OpenAPI\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\OpenAPI\\": "tests"
    }
  }
}